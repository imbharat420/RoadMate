{
  "name": "ReactNative3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "rn": "react-native",
    "start": "react-native start",
    "android": "react-native run-android",
    "android-dev": "react-native run-android --variant=release",
    "android-staging": "cross-env-shell ENVFILE=.env.staging react-native run-android --variant=release",
    "android-release": "cross-env-shell ENVFILE=.env.production react-native run-android --variant=release",
    "android-clean": "cd android && ./gradlew clean",
    "android-debug": "adb logcat *:E",
    "android-reconnect": "adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "ios-dev": "react-native run-ios --variant=release",
    "ios-staging": "cross-env-shell ENVFILE=.env.staging react-native run-ios --variant=release",
    "ios-release": "cross-env-shell ENVFILE=.env.production react-native run-ios --variant=release",
    "ios-clean": "cd ios && xcodebuild clean",
    "graphqlgen": "graphql-codegen --config codegen.yml",
    "templategen": "node ./templates/generate.js",
    "test": "echo \"No test specified\" && exit 0",
    "eslint": "eslint . --ext .ts,.tsx,.js --max-warnings 0 --config ./.eslintrc",
    "prettier": "prettier **/*.json --config ./.prettierrc",
    "lint": "yarn eslint && yarn prettier",
    "lint-fix": "yarn eslint --fix && yarn prettier --write",
    "rn-version": "react-native-version",
    "fix-lockfile": "node ./scripts/fix-lockfile.js",
    "postinstall": "patch-package && npx jetify"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.2",
    "react-native-eject": "^0.2.0",
    "react-native-paper": "^5.2.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
